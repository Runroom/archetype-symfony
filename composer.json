{
    "name": "runroom/archetype-symfony",
    "description": "This is our Symfony project template",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Runroom",
            "email": "runroom@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "homepage": "https://github.com/Runroom/archetype-symfony",
    "require": {
        "php": "^8.1",
        "a2lix/auto-form-bundle": "0.4.3",
        "a2lix/translation-form-bundle": "3.2.1",
        "doctrine/doctrine-bundle": "2.7.0",
        "doctrine/doctrine-fixtures-bundle": "3.4.2",
        "doctrine/doctrine-migrations-bundle": "3.2.2",
        "doctrine/orm": "2.12.3",
        "friendsofsymfony/ckeditor-bundle": "2.4.0",
        "gedmo/doctrine-extensions": "3.8.0",
        "jaybizzle/crawler-detect": "1.2.111",
        "knplabs/doctrine-behaviors": "2.6.1",
        "runroom-packages/basic-page-bundle": "0.15.7",
        "runroom-packages/ckeditor-sonata-media-bundle": "0.15.7",
        "runroom-packages/cookies-bundle": "0.15.7",
        "runroom-packages/redirection-bundle": "0.15.7",
        "runroom-packages/seo-bundle": "0.15.7",
        "runroom-packages/sortable-behavior-bundle": "0.15.7",
        "runroom-packages/translation-bundle": "0.15.7",
        "runroom/samples-bundle": "0.10.3",
        "sonata-project/admin-bundle": "4.16.0",
        "sonata-project/block-bundle": "4.16.0",
        "sonata-project/doctrine-extensions": "1.17.0",
        "sonata-project/doctrine-orm-admin-bundle": "4.3.3",
        "sonata-project/exporter": "2.13.0",
        "sonata-project/form-extensions": "1.18.0",
        "sonata-project/media-bundle": "4.4.0",
        "sonata-project/twig-extensions": "1.12.0",
        "sonata-project/user-bundle": "5.3.2",
        "symfony/doctrine-messenger": "6.1.3",
        "symfony/dotenv": "6.1.0",
        "symfony/flex": "2.2.2",
        "symfony/framework-bundle": "6.1.3",
        "symfony/mailer": "6.1.3",
        "symfony/messenger": "6.1.3",
        "symfony/monolog-bundle": "3.8.0",
        "symfony/runtime": "6.1.3",
        "symfony/security-bundle": "6.1.3",
        "symfony/twig-bundle": "6.1.1",
        "symfony/webpack-encore-bundle": "1.15.1",
        "symfonycasts/reset-password-bundle": "1.14.0",
        "twig/extra-bundle": "3.4.0",
        "twig/string-extra": "3.4.0",
        "twig/twig": "3.4.1",
        "zenstruck/foundry": "1.21.0"
    },
    "require-dev": {
        "deployer/dist": "6.8.0",
        "ergebnis/composer-normalize": "2.28.3",
        "friendsofphp/php-cs-fixer": "3.9.5",
        "phpstan/phpstan": "1.8.2",
        "phpstan/phpstan-doctrine": "1.3.11",
        "phpstan/phpstan-phpunit": "1.1.1",
        "phpstan/phpstan-symfony": "1.2.8",
        "phpunit/phpunit": "9.5.21",
        "psalm/plugin-phpunit": "0.17.0",
        "psalm/plugin-symfony": "3.1.8",
        "rector/rector": "0.13.9",
        "runroom-packages/testing": "0.15.7",
        "symfony/debug-bundle": "6.1.3",
        "symfony/maker-bundle": "1.45.0",
        "symfony/phpunit-bridge": "6.1.3",
        "symfony/stopwatch": "6.1.0",
        "symfony/web-profiler-bundle": "6.1.2",
        "theofidry/alice-data-fixtures": "1.6.0",
        "vimeo/psalm": "4.26.0",
        "weirdan/doctrine-psalm-plugin": "2.3.0",
        "zenstruck/browser": "1.1.0"
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "ergebnis/composer-normalize": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^5.4 || ^6.0"
        }
    },
    "scripts": {
        "lint-container": "@php bin/console lint:container",
        "lint-twig": "@php bin/console lint:twig templates",
        "lint-yaml": "@php bin/console lint:yaml config src translations --parse-tags",
        "php-cs-fixer": "@php php-cs-fixer fix --ansi --verbose",
        "phpstan": "@php phpstan analyse --no-progress",
        "psalm": "@php psalm --stats",
        "rector": "@php rector --no-progress-bar"
    }
}
