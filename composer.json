{
    "name": "runroom/archetype-symfony",
    "type": "project",
    "description": "This is our Symfony project template",
    "homepage": "https://github.com/Runroom/archetype-symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "runroom@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "a2lix/translation-form-bundle": "3.0.7",
        "doctrine/doctrine-bundle": "2.4.2",
        "doctrine/doctrine-migrations-bundle": "3.1.1",
        "doctrine/orm": "2.9.5",
        "friendsofsymfony/ckeditor-bundle": "2.3.0",
        "friendsofsymfony/user-bundle": "2.2.0",
        "gedmo/doctrine-extensions": "3.1.0",
        "jaybizzle/crawler-detect": "1.2.106",
        "knplabs/doctrine-behaviors": "2.2.0",
        "runroom-packages/basic-page-bundle": "0.13.0",
        "runroom-packages/ckeditor-sonata-media-bundle": "0.13.0",
        "runroom-packages/cookies-bundle": "0.13.0",
        "runroom-packages/redirection-bundle": "0.13.0",
        "runroom-packages/seo-bundle": "0.13.0",
        "runroom-packages/sortable-behavior-bundle": "0.13.0",
        "runroom-packages/translation-bundle": "0.13.0",
        "runroom/samples-bundle": "0.7.0",
        "sonata-project/admin-bundle": "3.105.0",
        "sonata-project/block-bundle": "3.23.2",
        "sonata-project/doctrine-extensions": "1.13.1",
        "sonata-project/doctrine-orm-admin-bundle": "3.35.0",
        "sonata-project/exporter": "2.7.0",
        "sonata-project/form-extensions": "1.9.0",
        "sonata-project/media-bundle": "3.32.0",
        "sonata-project/twig-extensions": "1.7.0",
        "sonata-project/user-bundle": "4.x-dev",
        "symfony/doctrine-messenger": "5.3.7",
        "symfony/dotenv": "5.3.7",
        "symfony/flex": "1.15.1",
        "symfony/framework-bundle": "4.4.30",
        "symfony/mailer": "5.3.4",
        "symfony/messenger": "5.3.7",
        "symfony/monolog-bundle": "3.7.0",
        "symfony/security-bundle": "4.4.27",
        "symfony/twig-bundle": "4.4.30",
        "symfony/webpack-encore-bundle": "1.12.0",
        "twig/extra-bundle": "3.3.1",
        "twig/string-extra": "3.3.1",
        "twig/twig": "2.14.6"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "require-dev": {
        "deployer/dist": "6.8.0",
        "doctrine/doctrine-fixtures-bundle": "3.4.0",
        "ergebnis/composer-normalize": "2.15.0",
        "friendsofphp/php-cs-fixer": "3.1.0",
        "phpstan/phpstan": "0.12.96",
        "phpstan/phpstan-doctrine": "0.12.44",
        "phpstan/phpstan-phpunit": "0.12.22",
        "phpstan/phpstan-symfony": "0.12.42",
        "phpunit/phpunit": "9.5.9",
        "psalm/plugin-phpunit": "0.16.1",
        "psalm/plugin-symfony": "3.0.2",
        "runroom-packages/testing": "0.13.0",
        "symfony/debug-bundle": "4.4.27",
        "symfony/maker-bundle": "1.33.0",
        "symfony/phpunit-bridge": "5.3.7",
        "symfony/web-profiler-bundle": "4.4.28",
        "theofidry/alice-data-fixtures": "1.4.0",
        "vimeo/psalm": "4.9.3",
        "weirdan/doctrine-psalm-plugin": "1.1.0"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^4.4 || ^5.3"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint-twig": "bin/console lint:twig templates",
        "lint-yaml": "bin/console lint:yaml config src translations --parse-tags",
        "php-cs-fixer": "php-cs-fixer fix --ansi --verbose",
        "phpstan": "phpstan analyse --no-progress",
        "psalm": "psalm --stats"
    }
}
