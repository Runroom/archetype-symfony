{
    "name": "runroom/archetype-symfony",
    "type": "project",
    "description": "This is our Symfony project template",
    "homepage": "https://github.com/Runroom/archetype-symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "runroom@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "a2lix/translation-form-bundle": "3.0.7",
        "doctrine/doctrine-bundle": "2.2.2",
        "doctrine/doctrine-migrations-bundle": "3.0.2",
        "doctrine/orm": "2.7.5",
        "friendsofsymfony/ckeditor-bundle": "2.2.0",
        "friendsofsymfony/user-bundle": "2.1.2",
        "gedmo/doctrine-extensions": "3.0.0",
        "jaybizzle/crawler-detect": "1.2.103",
        "knplabs/doctrine-behaviors": "2.0.8",
        "runroom-packages/basic-page-bundle": "0.9.2",
        "runroom-packages/ckeditor-sonata-media-bundle": "0.9.2",
        "runroom-packages/cookies-bundle": "0.9.2",
        "runroom-packages/form-handler-bundle": "0.9.2",
        "runroom-packages/redirection-bundle": "0.9.2",
        "runroom-packages/render-event-bundle": "0.9.2",
        "runroom-packages/seo-bundle": "0.9.2",
        "runroom-packages/sortable-behavior-bundle": "0.9.2",
        "runroom-packages/translation-bundle": "0.9.2",
        "runroom/samples-bundle": "0.3.1",
        "sonata-project/admin-bundle": "3.87.0",
        "sonata-project/block-bundle": "3.21.0",
        "sonata-project/doctrine-extensions": "1.11.0",
        "sonata-project/doctrine-orm-admin-bundle": "3.26.0",
        "sonata-project/exporter": "2.5.0",
        "sonata-project/form-extensions": "1.8.1",
        "sonata-project/media-bundle": "3.29.0",
        "sonata-project/twig-extensions": "1.5.0",
        "sonata-project/user-bundle": "4.10.1",
        "symfony/doctrine-messenger": "5.2.1",
        "symfony/dotenv": "5.2.1",
        "symfony/flex": "1.11.0",
        "symfony/framework-bundle": "4.4.18",
        "symfony/mailer": "5.2.1",
        "symfony/messenger": "5.2.1",
        "symfony/monolog-bundle": "3.6.0",
        "symfony/security-bundle": "4.4.18",
        "symfony/twig-bundle": "4.4.18",
        "symfony/webpack-encore-bundle": "1.8.0",
        "twig/extra-bundle": "3.1.1",
        "twig/string-extra": "3.2.1",
        "twig/twig": "2.14.3"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*"
    },
    "require-dev": {
        "deployer/dist": "6.8.0",
        "doctrine/doctrine-fixtures-bundle": "3.4.0",
        "ergebnis/composer-normalize": "2.13.2",
        "friendsofphp/php-cs-fixer": "2.17.3",
        "phpstan/phpstan": "0.12.65",
        "phpstan/phpstan-doctrine": "0.12.28",
        "phpstan/phpstan-phpunit": "0.12.17",
        "phpstan/phpstan-symfony": "0.12.14",
        "phpunit/phpunit": "9.5.0",
        "psalm/plugin-phpunit": "0.15.0",
        "psalm/plugin-symfony": "2.1.1",
        "runroom-packages/testing": "0.9.2",
        "symfony/debug-bundle": "4.4.18",
        "symfony/maker-bundle": "1.27.0",
        "symfony/phpunit-bridge": "5.2.1",
        "symfony/web-profiler-bundle": "4.4.18",
        "theofidry/alice-data-fixtures": "1.3.3",
        "vimeo/psalm": "4.4.1",
        "weirdan/doctrine-psalm-plugin": "1.0.0"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^4.4 || ^5.2"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "assets:install %PUBLIC_DIR% --symlink": "symfony-cmd"
        },
        "lint-twig": "bin/console lint:twig templates",
        "lint-yaml": "bin/console lint:yaml config src translations --parse-tags",
        "php-cs-fixer": "php-cs-fixer fix --ansi --verbose",
        "phpstan": "phpstan analyse --no-progress",
        "psalm": "psalm --stats"
    }
}
