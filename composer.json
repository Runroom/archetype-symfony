{
    "name": "runroom/archetype-symfony",
    "type": "project",
    "description": "This is our Symfony project template",
    "homepage": "https://github.com/Runroom/archetype-symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "runroom@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "a2lix/translation-form-bundle": "3.0.7",
        "doctrine/doctrine-bundle": "2.5.0",
        "doctrine/doctrine-migrations-bundle": "3.2.1",
        "doctrine/orm": "2.10.2",
        "friendsofsymfony/ckeditor-bundle": "2.3.0",
        "gedmo/doctrine-extensions": "3.3.1",
        "jaybizzle/crawler-detect": "1.2.108",
        "knplabs/doctrine-behaviors": "2.4.0",
        "runroom-packages/basic-page-bundle": "0.14.1",
        "runroom-packages/ckeditor-sonata-media-bundle": "0.14.1",
        "runroom-packages/cookies-bundle": "0.14.1",
        "runroom-packages/redirection-bundle": "0.14.1",
        "runroom-packages/seo-bundle": "0.14.1",
        "runroom-packages/sortable-behavior-bundle": "0.14.1",
        "runroom-packages/translation-bundle": "0.14.1",
        "runroom-packages/user-bundle": "0.14.1",
        "runroom/samples-bundle": "0.8.0",
        "sonata-project/admin-bundle": "4.3.1",
        "sonata-project/block-bundle": "4.8.0",
        "sonata-project/doctrine-extensions": "1.15.0",
        "sonata-project/doctrine-orm-admin-bundle": "4.2.0",
        "sonata-project/exporter": "2.9.1",
        "sonata-project/form-extensions": "1.12.1",
        "sonata-project/media-bundle": "^4.0.0@dev",
        "sonata-project/twig-extensions": "1.9.0",
        "symfony/doctrine-messenger": "5.3.10",
        "symfony/dotenv": "5.3.10",
        "symfony/flex": "1.17.2",
        "symfony/framework-bundle": "5.3.11",
        "symfony/mailer": "5.3.9",
        "symfony/messenger": "5.3.11",
        "symfony/monolog-bundle": "3.7.1",
        "symfony/runtime": "5.3.11",
        "symfony/security-bundle": "5.3.11",
        "symfony/twig-bundle": "5.3.10",
        "symfony/webpack-encore-bundle": "1.13.0",
        "symfonycasts/reset-password-bundle": "1.10.0",
        "twig/extra-bundle": "3.3.3",
        "twig/string-extra": "3.3.3",
        "twig/twig": "3.3.3"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "conflict": {
        "symfony/http-kernel": "5.3.7"
    },
    "require-dev": {
        "deployer/dist": "6.8.0",
        "doctrine/doctrine-fixtures-bundle": "3.4.1",
        "ergebnis/composer-normalize": "2.16.0",
        "friendsofphp/php-cs-fixer": "3.3.2",
        "phpstan/phpstan": "1.2.0",
        "phpstan/phpstan-doctrine": "1.0.1",
        "phpstan/phpstan-phpunit": "1.0.0",
        "phpstan/phpstan-symfony": "1.0.1",
        "phpunit/phpunit": "9.5.10",
        "psalm/plugin-phpunit": "0.16.1",
        "psalm/plugin-symfony": "3.0.4",
        "runroom-packages/testing": "0.14.1",
        "symfony/debug-bundle": "5.3.4",
        "symfony/maker-bundle": "1.36.2",
        "symfony/phpunit-bridge": "5.3.11",
        "symfony/stopwatch": "5.3.4",
        "symfony/web-profiler-bundle": "5.3.8",
        "theofidry/alice-data-fixtures": "1.4.0",
        "vimeo/psalm": "4.13.0",
        "weirdan/doctrine-psalm-plugin": "2.0.1"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "^5.3"
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "lint-twig": "bin/console lint:twig templates",
        "lint-yaml": "bin/console lint:yaml config src translations --parse-tags",
        "php-cs-fixer": "php-cs-fixer fix --ansi --verbose",
        "phpstan": "phpstan analyse --no-progress",
        "psalm": "psalm --stats"
    }
}
