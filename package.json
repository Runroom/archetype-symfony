{
  "name": "archetype-symfony",
  "version": "3.0.0",
  "description": "Runroom Symfony archetype",
  "author": "@runroom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Runroom/archetype-symfony.git"
  },
  "private": true,
  "scripts": {
    "build": "run-p build:* ckeditor:install",
    "build:encore": "encore production",
    "ckeditor:install": "cp -R node_modules/ckeditor4 public/ckeditor",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint assets/js",
    "lint:prettier": "prettier --check .github config assets translations webpack.config.js babel.config.js .eslintrc.js stylelint.config.js postcss.config.js prettier.config.js docker-compose.yaml",
    "lint:stylelint": "stylelint assets/css",
    "lint:tsc": "tsc --noEmit --pretty false",
    "start": "run-p start:* ckeditor:install",
    "start:encore": "encore dev",
    "watch": "run-p watch:* ckeditor:install",
    "watch:encore": "encore dev --watch"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@runroom/purejs": "3.1.1",
    "ckeditor4": "4.20.0",
    "jquery": "3.6.1",
    "jquery-validation": "1.19.5",
    "js-cookie": "3.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.20.2",
    "@babel/eslint-parser": "7.19.1",
    "@babel/preset-env": "7.20.2",
    "@runroom/npm-scripts": "1.2.10",
    "@symfony/webpack-encore": "4.1.2",
    "@types/jquery": "3.5.14",
    "@types/jquery.validation": "1.16.7",
    "@types/js-cookie": "3.0.2",
    "@typescript-eslint/eslint-plugin": "5.44.0",
    "@typescript-eslint/parser": "5.44.0",
    "autoprefixer": "10.4.13",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-webpack-plugin": "3.2.0",
    "file-loader": "6.2.0",
    "fork-ts-checker-webpack-plugin": "7.2.13",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.19",
    "postcss-at-rules-variables": "0.3.0",
    "postcss-functions": "4.0.2",
    "postcss-import": "15.0.0",
    "postcss-loader": "7.0.1",
    "postcss-mixins": "9.0.4",
    "postcss-nested": "5.0.6",
    "postcss-simple-vars": "7.0.1",
    "postcss-sort-media-queries": "4.3.0",
    "postcss-space": "1.0.0",
    "prettier": "2.8.0",
    "stylelint": "14.15.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-prettier": "2.0.0",
    "stylelint-webpack-plugin": "3.3.0",
    "tailwindcss": "3.2.4",
    "ts-loader": "9.4.1",
    "typescript": "4.8.4",
    "webpack": "5.75.0",
    "webpack-cli": "4.10.0",
    "webpack-notifier": "1.15.0"
  }
}
