{
  "name": "archetype-symfony",
  "version": "3.0.0",
  "description": "Runroom Symfony archetype",
  "author": "@runroom",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:Runroom/archetype-symfony.git"
  },
  "private": true,
  "scripts": {
    "start": "encore dev",
    "tokens:figma": "rm -rf tokens && bin/figma-tokens",
    "tokens:build": "style-dictionary build --config ./style-dictionary.json",
    "tokens": "npm run tokens:figma && npm run tokens:build",
    "watch": "yarn encore dev --watch"
  },
  "engines": {
    "node": ">=10.15"
  },
  "dependencies": {
    "@runroom/purejs": "3.1.0",
    "ckeditor4": "4.14.0",
    "jquery": "3.5.1",
    "jquery-validation": "1.19.2",
    "js-cookie": "2.2.1",
    "picturefill": "3.0.3"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/preset-env": "7.10.2",
    "@babel/register": "7.10.1",
    "@runroom/eslint-config-runroom": "1.1.0",
    "@runroom/stylelint-config-runroom": "1.3.0",
    "@symfony/webpack-encore": "0.30.2",
    "autoprefixer": "9.8.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "css-mqpacker": "7.0.0",
    "cssnano": "4.1.10",
    "eslint": "7.1.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-standard": "4.0.1",
    "esm": "^3.2.25",
    "file-system": "^2.2.2",
    "glob-all": "3.2.1",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "postcss-loader": "3.0.0",
    "sass-loader": "8.0.2",
    "stylelint": "13.6.0",
    "node-fetch": "^2.6.0",
    "purgecss-webpack-plugin": "2.1.2",
    "style-dictionary": "^2.8.3",
    "stylelint-webpack-plugin": "2.0.0",
    "terser-webpack-plugin": "3.0.3",
    "webpack-notifier": "1.8.0"
  }
}
